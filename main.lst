000008r 2               
000008r 2               CGADDR = $2121
000008r 2               CGDATA = $2122  ; 5-bit BGR, write twice, low byte first
000008r 2               CGDATARD = $213B  ; 5-bit BGR, read twice, low byte first
000008r 2               .define RGB(r,g,b) ((r)|((g)<<5)|((b)<<10))
