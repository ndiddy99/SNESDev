000154r 2               .macro LoadSprite sprite, xOffset, yOffset, tileNum, attributes, xBit, size
000154r 2               ;parameters: sprite num, pointer to x coord, pointer to y coord, pointer to tile num, attributes,first bit of x coordinate, big/small
000154r 2               ;shoutout to nintendo for making me go through all this bullshit, can't have
000154r 2               ; all the memory together or something sane
000154r 2               	lda sprite
000154r 2               	sta spriteNum
000154r 2               	rep #$20
000154r 2               	.a16
000154r 2               	lda sprite
000154r 2               	clc
000154r 2               	rol a
000154r 2               	rol a ;multiply sprite num by 4 because each index in oam table is 4 bytes
000154r 2               	tax
000154r 2               	sep #$20 ;8 bit a
000154r 2               	.a8
000154r 2               	lda xOffset
000154r 2               	sta OamMirror,x
000154r 2               	inx
000154r 2               	lda yOffset
000154r 2               	sta OamMirror,x
000154r 2               	inx
000154r 2               	lda tileNum
000154r 2               	sta OamMirror,x
