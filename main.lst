000154r 2               
000154r 2               	lda xBit
000154r 2               	and #$1 ;make sure only 1 bit
000154r 2               	sta oam2Data ;mess around with first bit of x coordinate b/c nintendo stored it separately
000154r 2               	lda size
000154r 2               	and #$1 ;combine sprite size and msb of xpos
000154r 2               	ror a
000154r 2               	ora oam2Data
000154r 2               	sta oam2Data
000154r 2               	lda spriteNum
000154r 2               	clc
000154r 2               	ror a ;4 sprites per oam table byte
000154r 2               	ror a
000154r 2               	and #$7F
000154r 2               	sta oam2WriteIndex
